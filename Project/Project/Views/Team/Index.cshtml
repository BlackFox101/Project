@model IEnumerable<Project.Models.Team>
@{
    ViewBag.Title = "Все команды";
}
<a asp-action="Create">Создать команду</a>
<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>
                <a asp-action="Details" asp-controller="Team" asp-route-id="@item.Id">Добавить участника</a> |
                <a asp-action="Delete" asp-controller="Team" asp-route-id="@item.Id">Удалить</a>
            </td>
        </tr>
    }
</table>
