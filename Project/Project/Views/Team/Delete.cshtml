@model Project.Models.Team
@{
    ViewBag.Title = "Удаление команды";
}

<div>
    <dl class="dl-horizontal">
        <dt>Команда</dt>
        <dd> @Html.DisplayFor(model => model.Title)</dd>

    </dl>

    <div>
        <form asp-controller="Team" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
